{
  "manifest_version": 3,
  "name": "Webtime Trackshare",
  "version": "1.0.0",
  "action": {
      "default_icon": {
          "16": "assets/temporary-icon-16px.png",
          "24": "assets/temporary-icon-24px.png",
          "32": "assets/temporary-icon-32px.png",
          "64": "assets/temporary-icon-64px.png",
          "128": "assets/temporary-icon-128px.png"
      },
      "default_title": "Webtime Trackshare",
      "default_popup": "index.html"
  },
  "description": "Webtime tracker with sharing functionality",
  "icons": {
      "16": "assets/temporary-icon-16px.png",
      "24": "assets/temporary-icon-24px.png",
      "32": "assets/temporary-icon-32px.png",
      "64": "assets/temporary-icon-64px.png",
      "128": "assets/temporary-icon-128px.png"
  },
  "author": "Darkhan Tleuzhan",
  "background": {
      "service_worker": "background.js"
  },
  "host_permissions": ["https://*/", "http://*/", "<all_urls>"],
  "incognito": "split",
  "permissions": ["scripting", "tabs", "storage", "idle"]
}